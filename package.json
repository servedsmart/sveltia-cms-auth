{
  "name": "sveltia-cms-auth",
  "version": "0.1.0",
  "license": "MIT",
  "type": "module",
  "private": true,
  "scripts": {
    "check": "pnpm run '/^check:.*/'",
    "check:audit": "pnpm audit",
    "check:cspell": "cspell --no-progress",
    "check:prettier": "prettier --check .",
    "check:eslint": "eslint .",
    "deploy": "NODE_ENV=production npx -y wrangler deploy",
    "dev": "npx -y wrangler dev",
    "start": "npx -y wrangler dev"
  },
  "devDependencies": {
    "@types/node": "^24.0.7",
    "cspell": "^9.1.2",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-jsdoc": "^51.2.3",
    "prettier": "^3.6.2",
    "wrangler": "4.22.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ],
    "overrides": {
      "esbuild@<=0.24.2": ">=0.25.0"
    }
  }
}
